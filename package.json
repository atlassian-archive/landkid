{
  "name": "landkid",
  "version": "0.0.1",
  "description": "CI Queue",
  "main": "index.js",
  "contributors": [
    "James Kyle <me@thejameskyle.com>",
    "Luke Batchelor <batchelor.luke@gmail.com>"
  ],
  "license": "MIT",
  "bin": {
    "landkid": "server.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "flow": "flow status",
    "test": "jest --coverage",
    "format": "prettier --write src/**/*.js",
    "build:legacy": "BABEL_ENV=legacy babel src -d dist/legacy -D",
    "build:modern": "BABEL_ENV=modern babel src -d dist/modern -D",
    "build": "yarn run clean && yarn build:legacy && yarn build:modern",
    "start": "node server.js | bunyan -o short",
    "prepublish": "yarn build",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "flow-bin": "^0.59.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "express": "^4.16.2",
    "joi": "^13.0.2",
    "load-json-file": "^4.0.0",
    "lockfile": "^1.0.3",
    "morgan": "^1.9.0",
    "path-exists": "^3.0.0",
    "redis": "^2.8.0",
    "strip-indent": "^2.0.0",
    "uuid": "^3.1.0",
    "write-json-file": "^2.3.0"
  },
  "jest": {
    "resetMocks": true,
    "testMatch": ["<rootDir>/src/**/__tests__/*.js"]
  },
  "lint-staged": {
    "*.js": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true
  }
}
